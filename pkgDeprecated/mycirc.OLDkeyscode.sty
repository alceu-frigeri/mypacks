\newcommand{\ORG@KeysDef}{\pgKeys{R} \pgKeys{L} \pgKeys{C} \pgKeys{Z} \pgKeys{K} \pgKeys[f_]{V} \pgKeys[f_]{I}  \pgKeys{T} \pgKeys{Q} \pgKeys{EQ} \pgKeys{X}  \pgKeys{D} \pgKeys{M} \pgKeys{N} \pgKeys{A} \pgKeys{B}
	\pgfkeys{/Zval/.is family, /Zval,
		default/.style = {/Zval/R, default, /Zval/L, default, /Zval/C, default, /Zval/Z, default, /Zval/K, default, /Zval/V, default, /Zval/I, default, /Zval/T, default, /Zval/Q, default,/Zval/EQ, default,/Zval/X, default, /Zval/D, default, /Zval/M, default, /Zval/N, default, /Zval/A, default, /Zval/B, default }}}%
%
%
%
%
\newcommand{\my@KeysetDef}[2][]{\pgKeys[#1]{#2}\pgfkeys{/Zval,default/.append style={/Zval/#2,default}}}
%
\newcommand{\pgKeys}[2][]{%
  \pgfkeys{/Zval/#2/.is family, /Zval/#2,default/.style = {}}%
	\gdef\my@KeyDefAux##1{\my@KeyDef{#1}{#2}{##1}}%
	\my@KeyDefLst{}%
	\forcsvlist\my@KeyDefLst{a,b,c,d}%,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z}%
}%
%
\gdef\my@KeyDefLst#1{\forcsvlist\my@KeyDefAux{#1a,#1b,#1c,#1d,#1e,#1f,#1g,#1h,#1i,#1j,#1k,#1l,#1m,#1n,#1o,#1p,#1q,#1r,#1s,#1t,#1u,#1v,#1w,#1x,#1y,#1z}}%
%
\newcommand{\my@KeyDef}[3]{\pgfkeys{/Zval/#2,%
    default/.append style = {#3=#2_{#1{#3}}},%
		#3/.code={\csedef{#2#3}{$##1$}}}%
}%
%
\newcommand{\TEMP@KeysDef}{%
  \pgfkeys{/Zval/.is family, /Zval, default/.style = {}}%
  \forcsvlist\my@KeysetDef{R,L,C,Z,K,T,Q,EQ,X,D,M,N,A,B,U}%
	\my@KeysetDef[f_]{V}\my@KeysetDef[f_]{I}%
}%
%
%
%
%
\newcommand{\ORG@pgPar}[1]{\pgfkeys{/Zval, default,  /Zval, #1}}%
%
%
%
\newcommand{\pgPar}[1]{\pgfkeys{/Zval, #1}}%
%
\newcommand{\@KeyDef}[3]{%
  \pgfkeysalso{%  /Zval/#2,%
		default/.append style = {#3=#2_{#1{#3}}},%
		#3/.code={\csedef{#2#3}{$##1$}}%
	}%
}%
%
\newcommand{\@SetDef}[2][]{%
  \pgfkeys{%
	  /Zval, default/.append style={/Zval/#2, default},%
	  /Zval/#2/.is family, /Zval/#2, default/.style={},%
		%keyset/.code={\@KeyDef{#1}{#2}{##1}},%
		keyset/.code={\pgfkeysalso{%  /Zval/#2,%
			default/.append style = {##1=#2_{#1{##1}}},%
			##1/.code={\csedef{#2##1}{$####1$}}%
		}},%
		setaux/.code={\pgfkeysalso{keyset/.list/.expanded={##1a,##1b,##1c,##1d,##1e,##1f,##1g,##1h,##1i,##1j,##1k,##1l,##1m,##1n,##1o,##1p,##1q,##1r,##1s,##1t,##1u,##1v,##1w,##1x,##1y,##1z}}},%
		setaux/.list/.expanded={,a,b,c,d,e}%
	}%
}%
%
\newcommand{\KeysDef}{%
  \pgfkeys{/Zval/.is family, /Zval, default/.style={},%
	  defset/.code={\@SetDef{##1}},%
		defset/.list/.expanded={R,L,C,Z,K,T,Q,EQ,X,D,M,N,A,B,U},%
		defset/.code={\@SetDef[f_]{##1}},%
		defset/.list/.expanded={V,I},%
		default%
	}%
}%  

